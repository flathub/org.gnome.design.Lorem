{
    "id": "org.gnome.design.Lorem",
    "runtime": "org.gnome.Platform",
    "runtime-version": "43",
    "sdk": "org.gnome.Sdk",
    "sdk-extensions": ["org.freedesktop.Sdk.Extension.rust-stable"],
    "command": "lorem",
    "finish-args": [
        "--device=dri",
        "--socket=fallback-x11",
        "--socket=wayland"
    ],
    "build-options": {
        "append-path": "/usr/lib/sdk/rust-stable/bin"
    },
    "cleanup": [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules": [
        {
            "name": "lorem",
            "buildsystem": "meson",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://gitlab.gnome.org/World/design/lorem/uploads/f877a94da78450db2b1725502f9be824/lorem-1.0.0.tar.xz",
                    "sha256": "e5eaf39a74007269b345233571051f93a11ec12b1f7aad2d873ac885267246d2"
                }
            ]
        }
    ]
}
